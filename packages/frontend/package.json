{
  "name": "culdevate-frontend",
  "version": "1.0.0",
  "description": "frontend UI for culdevate",
  "main": "src/Main.tsx",
  "scripts": {
    "setup": "npm ci",
    "build": "webpack -p --config ./webpack.config.prod.js",
    "dev": "webpack-dev-server --config ./webpack.config.dev.js -d --content-base ./dist",
    "prod": "webpack-dev-server --config ./webpack.config.prod.js",
    "test": "jest",
    "storybook": "start-storybook -p 9001",
    "lint": "tsc --noEmit && eslint --fix './src/**/*.{js,ts,jsx,tsx}'",
    "precommit": "tsc --noEmit && lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "keywords": [
    "culdevate-frontend",
    "frontend",
    "ui"
  ],
  "author": "alfienitylucero",
  "license": "ISC",
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-notes": "^5.2.4",
    "@storybook/addon-storysource": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.19",
    "@types/react": "^16.9.7",
    "@types/react-dom": "^16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
